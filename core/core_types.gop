package core

import "fmt"

func f_type (e *Env, args Value) (res Value, err error) {
  var vec []Value
  vec, ^err = UnpackArgs(args, 1, 1)
  switch it := vec[0].(type) {
  case *Cons:
    res = Intern("cons")
  case *Tagged:
    res = it.tag
  default:
    err = fmt.Errorf("Unknown type for %s", e)
  }
  return
}
